package com.kosa.diagram;

public class RestaurantTest {
	public static void main(String[] args) {
		Restaurant food1 = new Restaurant();
		food1.setFoodName("제육볶음");
		food1.setTime(15);
		food1.setPrice(9000);
		
		food1.order();
		food1.bill();

		Restaurant food2 = new Restaurant();
		food2.setFoodName("오징어볶음");
		food2.setTime(20);
		food2.setPrice(10000);
		
		food2.order();
		food2.bill();

		Restaurant food3 = new Restaurant();
		food3.setFoodName("라면");
		food3.setTime(5);
		food3.setPrice(4000);
		
		food3.order();
		food3.bill();
	}
}
