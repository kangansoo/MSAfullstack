package com.kosa.animalsimulator;

public class Dog extends Canine{

	public Dog(String picture, String food, String hunger, String boudaries, String location) {
		super(picture, food, hunger, boudaries, location);
		// TODO Auto-generated constructor stub
	}
	
	public void makeNoise() {
		System.out.println("개가 운다");
	}
	public void eat() {
		System.out.println("개가 먹는다");
	}
}
