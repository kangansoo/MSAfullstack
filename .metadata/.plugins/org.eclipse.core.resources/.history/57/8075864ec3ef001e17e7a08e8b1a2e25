package com.kosa.animalsimulator;

public class Tiger extends Feline {

	public Tiger(String picture, String food, String hunger, String boudaries, String location) {
		super(picture, food, hunger, boudaries, location);
		// TODO Auto-generated constructor stub
	}

	public void makeNoise() {
		System.out.println("호랑이가 운다");
	}
	public void eat() {
		System.out.println("호랑이가 먹는다");
	}
}
