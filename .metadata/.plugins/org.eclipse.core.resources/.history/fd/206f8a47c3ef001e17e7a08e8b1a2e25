package com.kosa.animalsimulator;

public class Wolf extends Canine{

	public Wolf(String picture, String food, String hunger, String boudaries, String location) {
		super(picture, food, hunger, boudaries, location);
		// TODO Auto-generated constructor stub
	}

	public void makeNoise() {
		System.out.println("늑대가 운다");
	}
	public void eat() {
		System.out.println("늑대가 먹는다");
	}
}
