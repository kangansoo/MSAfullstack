package com.kosa;

public class DooBee {
	public static void main(String[] args) {
			printDooBee(3);
			printLastDo(3);
		}
		static void printDooBee(int numberOfLoops) {
			int x = 1;
			while(x<numberOfLoops) {
				System.out.print("Doo");
				System.out.print("Bee");
				x += 1;
			}
		}
		static void printLastDo(int numberOfLoops) {
			if (numberOfLoops==3) {
				System.out.print("Do");
		}
	}
}
