package com.kosa;

import java.util.Scanner;

public class Loopy {
	
	public static void main(String[] args) {
		int x = 1;
		System.out.println("before the Loop");
		while(x < 4) {
			System.out.println("In the Loop");
			System.out.println("Value of x is "+x);
			x += 1;
		}
		System.out.println("This is after the loop");
	}

}
